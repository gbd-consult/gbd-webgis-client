{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "__": false
    },
    "extends": "eslint:recommended",
    "rules": {
        "require-await": "off",
        "no-unused-vars": "off",
        "no-console": "off",
        "no-extra-semi": "warn",
        "no-unreachable": "warn",
        "no-undef": "error"
    }
}
